<section class="content">
    <div class="header-dash" aria-owns="true">
        {{> headerDash}}
    </div>
    <hr>
    <div class="cards-content">
        {{#each chText}}
        <div class="cards">
            <div>
                <h3>{{this.name}}</h3>
                <h4>ID: {{this.id}}</h4>
                <h4>Posición: {{this.position}}</h4>
            </div>
        </div>
        {{/each}}
    </div>
</section>
