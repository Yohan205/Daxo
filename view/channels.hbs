<div class="dash-tittle">
    <div class="dash-img">
        <img src="https://cdn.discordapp.com/icons/{{servers.id}}/{{servers.icon}}.png" alt="icon{{servers.id}}">
    </div>
    <div>
        <h1><b>{{servers.name}}</b></h1>
        <h2>Lista de canales</h2>
    </div>
</div>
<hr>
{{#each canalesTxt}}
<div class="cards">
    <div class="cards-body">
        <h3 class="cards-title">Nombre: {{this.name}}</h3>
        <h4>ID: {{this.id}}</h4>
        <h4> Posición: {{this.position}}</h4>
    </div>
</div>
{{/each}}
</section>
<br>
