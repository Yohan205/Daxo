<div class="header-dash" aria-owns="true">
    {{> headerDash}}
</div>
<hr>
{{#each canalesTxt}}
<div class="cards">
    <div class="cards-body">
        <h3 class="cards-title">Nombre: {{this.name}}</h3>
        <h4>ID: {{this.id}}</h4>
        <h4> Posición: {{this.position}}</h4>
    </div>
</div>
{{/each}}
</section>
<br>
<section class="content">
    <div class="header-dash" aria-owns="true">
        {{> headerDash}}
    </div>
    <hr>
    {{#each canalesCty}}
    <div class="cards">
        <div class="cards-body">
            <h3 class="cards-title">Nombre: {{this.name}}</h3>
            <h4>ID: {{this.id}}</h4>
            <h4> Posición: {{this.position}}</h4>
        </div>
    </div>
    {{/each}}